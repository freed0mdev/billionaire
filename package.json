{
  "name": "billionare_game",
  "version": "0.1.0",
  "private": false,
  "homepage": "https://freed0mdev.github.io/billionaire",
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.7",
    "@testing-library/jest-dom": "^5.17.0",
    "classnames": "^2.3.2",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.19.0",
    "react-scripts": "5.0.1",
    "sass": "^1.69.5",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "start": "PUBLIC_URL=/ react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint": "eslint --ext .ts,.tsx src/",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "precommit": "lint-staged",
    "prepush": "npm test",
    "prepare": "npx husky install && npx husky add .husky/pre-commit 'npm run precommit' && npx husky add .husky/pre-push 'npm run prepush'"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": "npm run lint"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.64",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "@types/react-router-dom": "^5.3.3",
    "eslint": "^8.54.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "gh-pages": "^6.1.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.1.0"
  }
}
